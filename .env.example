# Sembalun Environment Variables Template
# Copy this file to .env.local for local development
# Copy this file to .env.production for production builds

# App Configuration
VITE_APP_NAME="Sembalun"
VITE_APP_VERSION="1.0.0"
VITE_APP_DESCRIPTION="Indonesian Meditation App"

# API Configuration (if needed for future features)
VITE_API_BASE_URL="https://api.sembalun.app"
VITE_API_TIMEOUT=10000

# Analytics (optional)
VITE_ANALYTICS_ID=""
VITE_GTM_ID=""

# PWA Configuration
VITE_PWA_THEME_COLOR="#6A8F6F"
VITE_PWA_BACKGROUND_COLOR="#E1E8F0"

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_OFFLINE_MODE=true

# Build Environment
NODE_ENV=production
VITE_BUILD_TIMESTAMP=