# Development Environment Variables
# Copy this file to .env.local and fill in your development Firebase project credentials



# App Configuration
VITE_APP_NAME="Sembalun Dev"
VITE_APP_VERSION="1.0.0-dev"
VITE_APP_DESCRIPTION="Indonesian Meditation App - Development"

# API Configuration
VITE_API_BASE_URL="http://localhost:5173"
VITE_API_TIMEOUT=10000

# Analytics & Performance (Development)
VITE_ANALYTICS_ID=""
VITE_GTM_ID=""

# Development Settings
VITE_USE_EMULATORS=true

# PWA Configuration
VITE_PWA_THEME_COLOR="#6A8F6F"
VITE_PWA_BACKGROUND_COLOR="#E1E8F0"

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_AUTH=true

# Build Environment
NODE_ENV=development