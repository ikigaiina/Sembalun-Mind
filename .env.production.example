# Sembalun Production Environment Variables
# Copy this file to .env.production and fill in your actual Firebase project credentials
# NEVER commit .env.production to version control



# App Configuration
VITE_APP_NAME="Sembalun"
VITE_APP_VERSION="1.0.0" 
VITE_APP_DESCRIPTION="Indonesian Meditation App"

# API Configuration
VITE_API_BASE_URL="https://your-domain.com"
VITE_API_TIMEOUT=10000

# Analytics & Performance

VITE_ANALYTICS_ID="your-analytics-id"
VITE_GTM_ID="your-gtm-id"

# Security Headers
VITE_ENABLE_CSP=true
VITE_ENABLE_SECURITY_HEADERS=true

# Development
VITE_USE_EMULATORS=false

# PWA Configuration
VITE_PWA_THEME_COLOR="#6A8F6F"
VITE_PWA_BACKGROUND_COLOR="#E1E8F0"

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_AUTH=true

# Build Environment
NODE_ENV=production